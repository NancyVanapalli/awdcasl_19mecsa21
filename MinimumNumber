import java.util.*;
class Minimum 
{
    public static void main(String[] args) 
  {
        int n,min;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
         a[i]=sc.nextInt();
         min=a[0];
         for(int i=1;i<n;i++)
       min=(int)(Math.min(a[i],min));
        System.out.println(min); 
   }
}
