import java.util.*;
class ProductOf_MinMax
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
a[i]=sc.nextInt();
int m=sc.nextInt();
int b[]=new int[m];
for(int i=0;i<m;i++)
b[i]=sc.nextInt();

int max=a[0],min=b[0];
for(int i=1;i<n;i++)
 max=(int)Math.max(a[i],max);
for(int i=1;i<m;i++)
min=(int)Math.min(b[i],min);
 System.out.println("product of max of 1st array & min of 2nd array=" + max*min);
}
}
