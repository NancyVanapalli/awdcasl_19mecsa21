import java.util.*;
class Total_length
 {
    public static void main(String[] args) 
   {
        int sum=0;
        String str;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the string data"); 
        str=sc.nextLine();
        String ch[]=str.split(" ");
        int n=ch.length;
        if(n==0)
        System.out.print(n);
        else
       {
         for(int i=0;i<n;i++)
         {
             String st=ch[i];
             int m=st.length();
             sum=sum+m;
         }
         System.out.println(sum);
       }
    }
 }
